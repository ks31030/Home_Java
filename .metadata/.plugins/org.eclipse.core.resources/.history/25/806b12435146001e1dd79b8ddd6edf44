package com.kh.practice2.func;

import java.util.Scanner;

public class CastingPractice1 {

	public static void main(String[] args) {
		Scanner sc = new scanner(System.in);
		char cname1, cname2;
		
		System.out.print("문자 : " + cname1);
		cname1 = sc.next();
		System.out.print("문자 : ");
		cname2 = sc.next();
		
	}

}
