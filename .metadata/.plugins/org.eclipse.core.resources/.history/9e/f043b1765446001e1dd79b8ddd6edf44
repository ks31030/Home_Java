package com.kh.practice2.func;

import java.util.*;

public class CastingPractice2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		double d1, d2, d3;
		
		System.out.print("국어 : ");
		d1 = sc.nextDouble();
		System.out.print("영어 : ");
		d2 = sc.nextDouble();
		System.out.print("수학 : ");
		d3 = sc.nextDouble();
	}

}
